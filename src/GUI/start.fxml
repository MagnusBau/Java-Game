<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.image.Image?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>


<AnchorPane prefHeight="500.0" prefWidth="800.0" xmlns="http://javafx.com/javafx/8.0.172-ea" xmlns:fx="http://javafx.com/fxml/1" fx:controller="GUI.StartController">
   <children>
      <ImageView fitHeight="500.0" fitWidth="800.0" pickOnBounds="true" preserveRatio="true">
         <image>
            <Image url="@images/Main.png" />
         </image>
      </ImageView>
      <Button fx:id="loginButton" layoutX="232.0" layoutY="432.0" mnemonicParsing="false" onAction="#login" prefHeight="47.0" prefWidth="149.0" text="Login">
         <font>
            <Font size="25.0" />
         </font>
      </Button>
      <Button fx:id="registerButton" layoutX="419.0" layoutY="432.0" mnemonicParsing="false" onAction="#register" prefHeight="47.0" prefWidth="149.0" text="Register">
         <font>
            <Font size="25.0" />
         </font>
      </Button>
   </children>
</AnchorPane>
